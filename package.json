{
  "name": "unist-util-inspect",
  "version": "4.0.0",
  "description": "Unist node inspector",
  "license": "MIT",
  "keywords": [
    "retext",
    "remark",
    "unist",
    "inspect"
  ],
  "repository": "https://github.com/syntax-tree/unist-util-inspect",
  "bugs": "https://github.com/syntax-tree/unist-util-inspect/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "is-empty": "1.1.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "chalk": "^1.0.0",
    "esmangle": "^1.0.1",
    "nyc": "^10.0.0",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "retext": "^4.0.0",
    "tape": "^4.4.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build-bundle": "browserify index.js --bare -s unistUtilInspect > unist-util-inspect.js",
    "build-mangle": "esmangle unist-util-inspect.js > unist-util-inspect.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "rules": {
      "unicorn/explicit-length-check": "off",
      "guard-for-in": "off"
    },
    "ignore": [
      "unist-util-inspect.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
