{
  "name": "retext-inspect",
  "version": "0.0.1",
  "description": "Retext node inspector",
  "license": "MIT",
  "keywords": [
    "retext",
    "inspect"
  ],
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "devDependencies": {
    "chalk": "^0.5.1",
    "eslint": "^0.8.0",
    "istanbul": "^0.3.0",
    "jscs": "^1.5.0",
    "mocha": "^1.20.0",
    "retext": "^0.2.0-rc.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/retext-inspect.git"
  },
  "scripts": {
    "test": "node_modules/.bin/_mocha --reporter spec --check-leaks -u exports spec/retext-inspect.spec.js",
    "test-travis": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --reporter spec --check-leaks -u exports spec/retext-inspect.spec.js",
    "lint": "npm run-script lint-api && npm run-script lint-test && npm run-script lint-style",
    "lint-api": "node_modules/.bin/eslint index.js --env node --env browser --rule 'quotes: [2, single]'",
    "lint-test": "node_modules/.bin/eslint spec/retext-inspect.spec.js --env node --env mocha --rule 'quotes: [2, single]'",
    "lint-style": "node_modules/.bin/jscs index.js spec/retext-inspect.spec.js --reporter=inline",
    "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -- spec/retext-inspect.spec.js",
    "make": "npm run-script lint && npm run-script coverage"
  }
}
